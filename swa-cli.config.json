{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "react": {
      "appLocation": "react-app/",
      "outputLocation": "build",
      "appBuildCommand": "npm run build",
      "run": "BROWSER=none npm start",
      "start": {
        "outputLocation": "http://localhost:3000"
      },
      "deploy": {
        "outputLocation": "build"
      }
    }
  }
}